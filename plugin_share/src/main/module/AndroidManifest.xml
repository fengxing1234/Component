<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.component.fx.plugin_share">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/share_ic_launcher"
        android:name=".ShareApplication"
        android:label="@string/login_app_name"
        android:roundIcon="@mipmap/share_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/login_AppTheme">
        <activity android:name=".PluginShareActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>